{% set title = "하소연" %}
{% extends "layout/base.html" %}

{% block style -%}
<link rel="stylesheet" href="/css/complain.css">
{% endblock %}
{% block content -%}
<div class="character-wrapper">
  <div class="main">
    <div class="monster">
     <div class="monster__hair"></div>
      <div class="monster__face">
        <div class="monster__eyes">
          <div class="monster__eye"></div>
          <div class="monster__eye"></div>
        </div>
        <div class="monster__cheek">
          <div class="monster__cheek__point"></div>
          <div class="monster__cheek__point"></div>
        </div>
        <div class="monster__mouth">
          <div class="monster__top"></div>
          <div class="monster__bottom"></div>
        </div>
      </div>
    </div>
    <div class="monster__bg shadow">
    </div>
  </div>
  <div class="self_video">
    
  </div>
  <div class="btn_wrapper">
    <div class="btn1  btn" data-value='1'>1</div>
    <div class="btn2  btn" data-value='2'>2</div>
    <div class="btn3  btn" data-value='3'>3</div>
    <div class="btn4  btn" data-value='4'>4</div>
    <div class="btn4  btn" data-value='5'>5</div>
    <div class="btn4  btn" data-value='6'>6</div>
    </div>
</div>
{% endblock %}

{% block script%}
<script defer src="/js/face-api.min.js"></script>
<script src="/js/complain.js" defer></script>

{% endblock %}